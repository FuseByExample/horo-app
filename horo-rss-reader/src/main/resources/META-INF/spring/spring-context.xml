<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:osgi="http://www.springframework.org/schema/osgi"
      xsi:schemaLocation="
      http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
      http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">

    <camelContext id="bookings" xmlns="http://camel.apache.org/schema/spring">
        <propertyPlaceholder id="properties" location="ref:osgi.properties"/>

        <route id="blogReader">
          <from uri="rss:http://feeds.feedburner.com/JakubKorab?format=xml" />
          <to uri="log:blogReader" />
        </route>
    </camelContext>

</beans>
